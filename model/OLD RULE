      提示: "隐藏一名或多名玩家的英雄HUD，直到重新被显示为止。",
    },

    隐藏计分板: {
      参数: {
        玩家: {
          选项: "条件",
          提示: "下列玩家的计分板将被隐藏。",
        },
      },
      标签: ["动作"],
      提示: "隐藏一名或多名玩家的消灭提示，直到重新被显示为止。",
    },

    预加载英雄: {
      参数: {
        玩家: {
